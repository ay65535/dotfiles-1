alias -s dco 'docker-compose'
alias -s dcb 'docker-compose build'
alias -s dce 'docker-compose exec'
alias -s dcps 'docker-compose ps'
alias -s dcrestart 'docker-compose restart'
alias -s dcrm 'docker-compose rm'
alias -s dcr 'docker-compose run'
alias -s dcstop 'docker-compose stop'
alias -s dcup 'docker-compose up'
alias -s dcupd 'docker-compose up -d'
alias -s dcdn 'docker-compose down'
alias -s dcl 'docker-compose logs'
alias -s dclf 'docker-compose logs -f'
alias -s dcpull 'docker-compose pull'
alias -s dcstart 'docker-compose start'
