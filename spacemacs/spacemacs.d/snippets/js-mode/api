# -*- mode: snippet -*-
# name : WSP API call
# key: api
# --

dispatch(loadingStart());

return Api.invoke({
  path: '${1:/path/to}',
  param: {
    $2
  },
})
    .then((body: ${3:ResponseBodyType}) => dispatch(${4:successAction}()))
    .catch((err: Error) => dispatch(catchApiError(err, { isContinuable: ${5:true} })))
    .then(() => dispatch(loadingEnd()));
$0
