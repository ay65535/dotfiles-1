const ${1:...}Selector = createSelector(
  (state) => state.${2:...}.allIds,
  (state) => state.$2.byId,
  (
    allIds: String[],
    byId: {
      [String]: ${3:...}
    }
  ) => allIds.map((id: String) => byId[id])
);$0
