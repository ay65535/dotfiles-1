#!/usr/bin/env bash

# sdk-wrapper
# author: Seong Yong-ju <sei40kr@gmail.com>

set -euo pipefail
IFS=$'\n\t'

: ${SDKMAN_DIR:=${HOME}/.sdkman}

if [[ ! -d "${SDKMAN_DIR}" ]]; then
    echo "ERROR: SDKMAN! is not found on SDKMAN_DIR: SDKMAN_DIR=${SDKMAN_DIR}"
    exit 1
fi

. "${SDKMAN_DIR}/bin/sdkman-init.sh" && sdk "$@"
