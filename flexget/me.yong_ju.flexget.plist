<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
    "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>me.yong_ju.flexget</string>
  <key>ProgramArguments</key>
  <array>
    <string>/bin/sh</string>
    <string>-c</string>
    <string>~/.pyenv/shims/flexget daemon start --autoreload-config</string>
  </array>
  <key>RunAtLoad</key>
  <true/>
  <key>StandardOutPath</key>
  <string>/tmp/me.yong_ju.flexget.stdout.log</string>
  <key>StandardErrorPath</key>
  <string>/tmp/me.yong_ju.flexget.stderr.log</string>
</dict>
</plist>
