<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
    "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.flexget</string>
  <key>ProgramArguments</key>
  <array>
    <string>~/.pyenv/shims/flexget</string>
    <string>daemon</string>
    <string>start</string>
    <string>--autoreload-config</string>
  </array>
  <key>RunAtLoad</key>
  <true/>
  <key>StandardOutPath</key>
  <string>/tmp/com.flexget.stdout.log</string>
  <key>StandardErrorPath</key>
  <string>/tmp/com.flexget.stderr.log</string>
</dict>
</plist>
