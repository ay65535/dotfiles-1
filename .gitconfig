[user]
  email = "sei40kr@gmail.com"
  name = "Seong Yong-ju"
[core]
  editor = nvim
  excludesfile = ~/.gitignore-global
[fetch]
  prune = true
[interactive]
  diffFilter = diff-highlight
[merge]
  tool = fugitive
[mergetool "fugitive"]
  cmd = "nvim -fc Gdiff $MERGED"
[pager]
  log = "diff-highlight | less"
  show = "diff-highlight | less"
  diff = "diff-highlight | less"
[push]
  default = current
[github]
  user = "sei40kr"
[ghq]
  root = ~/dev/ws

